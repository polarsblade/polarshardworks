account_information.Text = localplayer.Name.." / "..tostring(localplayer.UserId)
localplayer.CharacterAdded:Connect(function()
    deathcount += 1
end)

while farm_gui and task.wait() do
    task.wait()

    pcall(function()
        money_farmed.Text = "$"..format(datafolder:WaitForChild("Currency").Value - startingcash)
    end)

    pcall(function()
        total_cash.Text = "$"..format(datafolder:WaitForChild("Currency").Value)
    end)

    pcall(function()
        time_elapsed.Text = tostring(convert_to_hms(math.floor(tick() - timeelapsed)))
    end)

    pcall(function()
        death_count.Text = tostring(deathcount)
    end)
end
