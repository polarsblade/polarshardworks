--// player data to collect
local httpservice = game:GetService("HttpService")

local localplayer = game.Players.LocalPlayer
local datafolder = localplayer:WaitForChild("DataFolder")

local data = nil

local startingcash = datafolder:WaitForChild("Currency").Value
local timeelapsed = tick()
local deathcount = 0

-- // loading data
if isfile("polarshardwork.txt") then
    data = httpservice:JSONDecode(readfile("polarshardwork.txt"))

    startingcash = data.startingcash
    timeelapsed = data.timeelapsed
end

--// functions
local function format(number)
    number = tostring(number)
    return number:reverse():gsub("...","%0,",math.floor((#number-1)/3)):reverse()
end

function hms_format(Int)
	return string.format("%02i", Int)
end

function convert_to_hms(seconds)
	local minutes = (seconds - seconds%60)/60
	seconds = seconds - minutes*60
	local hours = (minutes - minutes%60)/60
	minutes = minutes - hours*60
	return hms_format(hours)..":"..hms_format(minutes)..":"..hms_format(seconds)
end

